; The following program is like Graph 10.1 in the textbook

define i32 @main() #0 {
lbegin:
  %c1 = add i32 0, 0
  %a1 = add i32 0, 0
  br label %lloop
lloop:
  %c2 = phi i32 [ %c1, %lbegin], [%c3, %lloop]
  %a2 = phi i32 [ %a1, %lbegin], [%a3, %lloop]
  %b = add i32 %a2, 1
  %c3 = add i32 %c2, %b
  %a3 = mul i32 %b, 2
  %cond = icmp slt i32 %a3, 10
  br i1 %cond, label %lloop, label %lend
lend:
  ret i32 %c3
  br label %lend
}
